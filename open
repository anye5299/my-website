function getQueryParam(variable)
{
  var query = window.location.search.substring(1);var vars = query.split("?");
  var codarr = vars[0].split("&");

  for (var i=0;i<codarr.length;i++) {
    // console.log(codarr);
      for (var i=0;i<codarr.length;i++) {var pair = codarr[i].split("=");
        if(pair[0] == variable){
          return pair[1];
        }
      }
  }

  return(false);
}


